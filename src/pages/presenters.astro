---
import Default from '../layouts/Default.astro';
import { getEntry, getCollection } from 'astro:content';
const presenters = await getCollection('presenters');
---

<Default>
	<article>
		<h2>Presenters Emeritus</h2>

		<ol class="presenters">
			{presenters.map(presenter => <li>{presenter.name}</li>)}
			<!--{% for presenter_array in site.data.presenters %}-->
			<!--	{% assign presenter = presenter_array[1] %}-->
			<!--	{% include bio.html presenter=presenter nowrap=true %}-->
			<!--{% endfor %}-->
		</ol>
		<p><em>Ordered by first presentation date.</em></p>
	</article>
</Default>
